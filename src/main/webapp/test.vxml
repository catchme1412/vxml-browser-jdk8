         <prompt>Who would win in a fist fight, Oprah, Madonna, or James Brown?</prompt>
         <grammar xml:lang="en-us" root="myrule">
            <rule id="myrule">
               <one-of>
                  <item>1</item>
                  <item>2</item>
                  <item>2</item>
               </one-of>
            </rule>
         </grammar>
         <filled>
